{
  "name": "data-ingest-lambda",
  "version": "1.0.0",
  "description": "Project to ingest .csv files to MSSQL Server RDS.",
  "main": "index.ts",
  "author": "Raghav Medapati",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && swc ./src -d ./dist --ignore \"**/*.test.ts,**/__test__/**\"",
    "test": "jest --clearCache --coverage",
    "lint": "eslint \"src/**/*.{json,js,jsx,ts,tsx}\" && tsc --noemit",
    "prettier": "prettier \"src/**/*.{json,js,jsx,ts,tsx}\" --write",
    "package": "mkdir -p dist/layers/deps-layer/nodejs && cp package*.json dist/layers/deps-layer/nodejs/ && cp yarn.lock dist/layers/deps-layer/nodejs/ && cd dist/layers/deps-layer/nodejs && yarn install --production --frozen-lockfile"
  },
  "devDependencies": {
    "@aws-sdk/client-rds-data": "^3.645.0",
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/client-secrets-manager": "^3.645.0",
    "@aws-sdk/client-sns": "^3.645.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.18",
    "@swc/jest": "^0.2.36",
    "@types/aws-lambda": "^8.10.143",
    "@types/jest": "^29.5.12",
    "@types/mssql": "^9.1.5",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^9.9.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "csv-parser": "^3.0.0",
    "mssql": "^11.0.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
