{
  "name": "data-ingest-lambda",
  "version": "1.0.0",
  "description": "Project to ingest .csv files to MSSQL Server RDS.",
  "main": "index.ts",
  "author": "Raghav Medapati",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && swc ./src -d ./dist --ignore \"**/*.test.ts,**/__test__/**\"",
    "test": "jest --clearCache --coverage",
    "lint": "eslint \"src/**/*.{json,js,jsx,ts,tsx}\" && tsc --noemit",
    "prettier": "prettier \"src/**/*.{json,js,jsx,ts,tsx}\" --write",
    "package": "mkdir -p dist/layers/deps-layer && cp package*.json dist/layers/deps-layer/ && cp yarn.lock dist/layers/deps-layer/ && cd dist/layers/deps-layer && yarn --frozen-lockfile"
  },
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.18",
    "@swc/jest": "^0.2.36",
    "@types/aws-lambda": "^8.10.143",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "aws-sdk": "^2.1682.0",
    "eslint": "^9.9.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "csv-parser": "^3.0.0",
    "tedious": "^18.6.1"
  }
}
